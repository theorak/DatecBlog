{
  "name": "datec/datec-blog",
  "type": "typo3-cms-extension",
  "description": "Adds a lightweight Blog to your Typo3 Site.",
  "keywords": [
    "blog",
    "light",
    "modern",
    "comment",
    "keywords",
    "datec"
  ],
  "homepage": "http://www.datec-schmidt.de/",
  "authors": [
    {
      "name": "Philipp Roensch",
      "email": "p.roensch@datec-schmidt.de",
      "role": "Developer",
      "homepage": "http://www.datec-schmidt.de/"
    }
  ],
  "support": {
    "issues": "https://github.com/theorak/datec_blog/issues"
  },
  "require": {
    "typo3/cms-core": ">=8.7,<9.0"
  },
  "autoload": {
    "psr-4": {
      "Datec\\DatecBlog\\": "Classes/"
    }
  },
  "autoload-dev": {
	"psr-4": {
      "Datec\\DatecBlog\\Tests\\": "Tests/"
	}
  }
}
